public class Peinture extends Revetement {
    
    int idPeinture;
    double prix;
    int cpeinture;

    public Peinture(int idPeinture, double prix, int idRevetement) {
        super(idRevetement);
        this.idPeinture = idPeinture;
        this.prix = prix;
    }
   
    
    
    public double montant (Mur.surface(),prix){
        double montant = Mur.surface() * prix;
        return montant;
    }
  }
